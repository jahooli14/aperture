{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron/weekly-synthesis",
      "schedule": "0 9 * * 1"
    },
    {
      "path": "/api/cron/strengthen-nodes",
      "schedule": "0 0 * * *"
    }
  ]
}
